<mvc:View
    controllerName="siar.controller.App"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core"
    xmlns="sap.m"
    xmlns:tnt="sap.tnt">
    <App>
        <pages>
            <Page class="customPage">
                <customHeader >
                    <core:Fragment fragmentName="siar.view.fragments.masterpage.Toolbar" type="XML" class="customHeader"/>
                </customHeader>
                <content>
                    <SplitApp id="splitApp" class="customSlipApp">
                        <masterPages>
                            <Page id="menuPage">
                                <tnt:SideNavigation width="100%" class="customListMenu">
                                    <tnt:NavigationList id="navigationList" >
                                    </tnt:NavigationList>
                                </tnt:SideNavigation>
                            </Page>
                        </masterPages>
                        <detailPages>
                            <Page id="Here"></Page>
                        </detailPages>
                    </SplitApp>
                </content>
            </Page>
        </pages>
    </App>
</mvc:View>


